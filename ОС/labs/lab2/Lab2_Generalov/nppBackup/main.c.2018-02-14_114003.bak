#include <conio.h>

int **randomFill(int ** arr, size_t nRow, size_t nCol){

	for (size_t i = 0; i < nRow; ++i)
		for(size_t j = 0; j < nCol; ++j){
			arr[i][j] = rand() % 999;
		}

	return arr;
}

